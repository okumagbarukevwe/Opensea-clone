      <div class="container width-red">
        <div class="row">
          <div class="col  tab button-left">
            <span class="big-p-table">10.0K</span><br>
            <span id="small-table">items</span>
          </div>
          <div class="col tab">
            <span class="big-p-table">3.4K</span><br>
            <span  id="small-table" >owners</span>
          </div>
          <div class="col tab">
            <span class="big-p-table"><img class="eth-svg" src="/eth.svg" alt="">&nbsp;&nbsp; ---</span><br>
            <span id="small-table">floor price</span>
          </div>
          <div class="col tab button-right">
            <span class="big-p-table"><img class="eth-svg" src="/eth.svg" alt="">&nbsp;&nbsp;850.4K</span><br>
